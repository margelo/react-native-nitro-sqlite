+  }
+  static inline jsi::Value toJSI(jsi::Runtime& runtime, const std::exception_ptr& exception) {
+    if (exception == nullptr) [[unlikely]] {
+      throw std::runtime_error("Cannot convert a nullptr exception_ptr to a JS Error!");
+    }
