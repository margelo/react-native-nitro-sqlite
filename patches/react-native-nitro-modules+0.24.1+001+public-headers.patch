+template <typename T>
+BorrowingReference<T> WeakReference<T>::lock() const {
+  std::unique_lock lock(_state->mutex);
+
+  if (_state->isDeleted) {
+    // return nullptr
+    return BorrowingReference<T>();
+  }
+
+  return BorrowingReference(*this);
+}
