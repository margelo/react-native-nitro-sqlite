+    _map = std::make_shared<AnyMap>();
+  }
+  JAnyMap(const std::shared_ptr<AnyMap>& map) : _map(map) {}
+
+protected:
+  bool contains(const std::string& key) {
+    return _map->contains(key);
+  }
