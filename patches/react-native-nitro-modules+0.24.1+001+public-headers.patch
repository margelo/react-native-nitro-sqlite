+  static inline int fromJSI(jsi::Runtime&, const jsi::Value& arg) {
+    return static_cast<int>(arg.asNumber());
+  }
+  static inline jsi::Value toJSI(jsi::Runtime&, int arg) {
+    return jsi::Value(arg);
+  }
