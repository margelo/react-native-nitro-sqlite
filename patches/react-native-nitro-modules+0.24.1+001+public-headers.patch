+protected:
+  double getDouble(const std::string& key) {
+    return _map->getDouble(key);
+  }
+  bool getBoolean(const std::string& key) {
+    return _map->getBoolean(key);
+  }
+  int64_t getBigInt(const std::string& key) {
+    return _map->getBigInt(key);
