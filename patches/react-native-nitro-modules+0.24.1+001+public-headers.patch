+// Created by Marc Rousavy on 21.02.24.
+//
+
+#pragma once
+
+// Forward declare a few of the common types that might have cyclic includes.
+namespace margelo::nitro {
+
+template <typename T, typename Enable>
+struct JSIConverter;
+} // namespace margelo::nitro
