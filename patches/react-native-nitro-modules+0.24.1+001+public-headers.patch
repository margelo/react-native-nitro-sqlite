+public:
+  explicit ByteBufferArrayBuffer(const jni::alias_ref<jni::JByteBuffer>& byteBuffer) : _byteBuffer(jni::make_global(byteBuffer)) {
+    _byteBuffer->order(jni::JByteOrder::nativeOrder());
+  }
